{
  "name": "chrome-extension-react-typescript-boilerplate",
  "version": "1.1.0",
  "description": "A boilerplate project to quickly build a Chrome extension using TypeScript and React (built using webpack).",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "dev": "webpack --config webpack.dev.js",
    "watch": "webpack -w --config webpack.dev.js",
    "contracts": "forge install && forge build && typechain --target=ethers-v5 'out/**/*.json'"
  },
  "author": "Joe Martella",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.0.60",
    "@types/node": "^18.11.2",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "css-loader": "^3.4.2",
    "node-sass": "^7.0.3",
    "sass-loader": "^13.1.0",
    "style-loader": "^0.20.3",
    "ts-loader": "^4.0.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.3.6",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@web3auth-mpc/openlogin-adapter": "^2.1.22",
    "@web3auth-mpc/web3auth": "^2.1.22",
    "add": "^2.0.6",
    "brfs": "^2.0.2",
    "browserify-fs": "^1.0.0",
    "crypto-browserify": "^3.12.0",
    "ethers": "^5.7.2",
    "extension-port-stream": "^2.0.1",
    "framer-motion": "^7.6.1",
    "https-browserify": "^1.0.0",
    "obj-multiplex": "^1.0.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "torus-mpc": "^0.0.10",
    "transform-loader": "^0.2.4",
    "url": "^0.11.0",
    "webextension-polyfill": "^0.10.0",
    "yarn": "^1.22.19"
  }
}
